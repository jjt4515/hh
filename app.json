    },
    "packagerOpts": {
      "port": 53114
    },
    "ios": {
      "bundleIdentifier": "com.jjt4515.fashionstyletest",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jjt4515.fashionstyletest",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    }
  }
}
